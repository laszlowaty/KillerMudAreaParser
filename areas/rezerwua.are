#klan_social_pro
Social z podanym celem. Nalezy w lini 1 wpisac nazwe sociala oraz w liniach: 20, 21, 31, 35, 37 wpisac opisy sociala. Podpinamy na trigger input "@^nazwa_sociala \w+$".
~
$_nazwa_sociala = "asd"

$_dlugosc_nazwy = str_length $_nazwa_sociala
$_dlugosc_nazwy = int_add $_dlugosc_nazwy 1
$_dlugosc = str_length $_0
$_dlugosc = int_sub $_dlugosc $_dlugosc_nazwy
$_0 = str_substr $_0 $_dlugosc_nazwy $_dlugosc
$_pokoj = room_by_char $n
if streval $_0 eq "self"
  $_cel = $n
else
  $_cel = char_in_room_by_name $_pokoj $_0
  if varisnull $_cel
    mob echoat $n Nie ma tu nikogo takiego.
    break
  endif
endif
mob setflag $n social_klanowy 1
if hasflag $_cel social_klanowy
  mob echoat $n Wiadomosc do wykonawcy, gdy celem jest 'self'.
  mob echoaround $n Wiadomosc do otoczenia, gdy celem jest 'self'.
  mob removeflag $n social_klanowy
  break
endif
$_start = char_first_in_room $_pokoj
:petla
if varisnull $_start
  break
endif
if hasflag $_start social_klanowy
  mob echoat $_start Wiadomosc do osoby, ktora wykonuje sociala.
  mob removeflag $n social_klanowy
else
  if streval $_start eq $_cel
    mob echoat $_start Wiadomosc do osoby docelowej.
  else
    mob echoat $_start Wiadomosc do otoczenia.
  endif
endif
$_start = char_next_in_list $_start
goto petla
