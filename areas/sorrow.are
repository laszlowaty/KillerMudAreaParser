$_comm        = "zag(r|ra|raj)? "
$_action      = "zagrac"
$_jest_karta  = 0
$_drawwartosc = 0
$_drawkolor   = 0
$_flag_name   = " "
$_char = $n
$_zagrane = obj_in_room_by_vnum (room_by_obj $o) 16844
if varisnull $_zagrane
else
  $_curr_flag = obj_first_flag $_zagrane
  :flagloop
  if varisnull $_curr_flag
  else
    $_chck = str_substr $_curr_flag 3 2
    if streval $_chck eq "_n"
      $_flag_lgh = str_length $_curr_flag
      $_name     = str_substr $_curr_flag 8 $_flag_lgh
      if streval $_name eq $_char.name
        mob echoat $n przecie¿ juz zagrywa³<&e/a/o>¶ karte do tej lewy.
      endif
    endif
    $_curr_flag = obj_next_flag $_zagrane $_curr_flag
    goto flagloop
  endif
endif
call objprog wachlarz_dcore
if numeval $_jest_karta == 1
  obj modflag mod $_flag_name -$_drawkolor
  obj modflag mod reg_ilosc_kart -1
  if objflag $o reg_ilosc_kart == 0
    mob echoat $n bierzesz ostatni± karte z prawej reki w której trzymal<&e/a/o>s wachlarzyk...
    mob echoaround $n $n bierze ostatni± karte z prawej reki trzymaj±cej wachlarzyk...
    $_delwach = 1
  else
    mob echoat $n wyci±gasz jedn± karte z trzymanego w reku wachlarzyka...
    mob echoaround $n $n wyci±ga jedn± karte z trzymanego w reku wachlarzyka...
    call objprog wachlarz_licz
  endif
  $_zagrane = obj_in_room_by_vnum (room_by_obj $o) 16844
  if varisnull $_zagrane
    mob echoat $n ... i zagrywasz j± przed soba, jako pierwsz± karte w lewie
    mob echoaround $n ... i zagrywa j± przed sob±, jako pierwsz± karte w lewie
    mob oload 16844 r
    $_zagrane = obj_in_room_by_vnum (room_by_obj $o) 16844
    mob setflag $_zagrane reg_ilosc_kart 1
  else
    mob echoat $n ... i zagrywasz j± do istniej±cej juz lewy
    mob echoaround $n ... i zagrywa j± do istniej±cej juz lewy
    mob modflag $_zagrane mod reg_ilosc_kart 1
  endif
  $_flag_count     = obj_flag_value $_zagrane "reg_ilosc_kart"
  $_flag_count_str = int_to_str $_flag_count
  $_flag_name = str_concat "reg_card_" $_flag_count_str
  $_card_val  = int_add (int_mult $_drawkolor 100) $_drawwartosc
  mob setflag $_zagrane $_flag_name $_card_val
  if numeval $_flag_count < 10
    $_flag_count_str = str_concat "0" $_flag_count_str
  endif
  $_flag_name = str_concat (str_concat "reg_n" (str_concat $_flag_count_str "_")) $_char.name
  mob setflag $_zagrane $_flag_name 1
  call objprog lewa_rysuj $_zagrane
else
  mob echoat $n chyba nie masz takiej karty na reku...
endif
if numeval $_delwach == 1
  obj remove
endif
